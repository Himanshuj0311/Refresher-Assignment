{
    "env": {
      "node": true,
      "es6": true
    },
    "extends": ["eslint:recommended", "plugin:node/recommended"],
    "plugins": ["node"],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"]
    }
  }
  